<!doctype html>

<html>
<head>
	<title>yoso</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
	<link href="css/bootstrap.css" rel="stylesheet">
	<link href="css/bootstrap-theme.css" rel="stylesheet">
	<link href="css/styles.css" rel="stylesheet">
</head>

<body>
	<header class="menu row container">
		<a href="#" class="left-nav col-xs-2">
			<span class="glyphicon glyphicon-chevron-left"></span>
		</a>
		<h1 class="title col-xs-8">yoso</h1>
	</header>

	<div class="top-offset"></div>

	<ul class="list-area list-all" id="lists">
		{{#each lists}}
		<li class="list-obj row container">
			<a href="lists.html" class="list-el">
				<text class="col-xs-10">{{name}}</text>
			</a>		
		</li>
		{{/each}}
	</ul>

	{{#each lists}}
	<ul class="list-area list-individual" id="{{name}}">
		{{#each contents}}
		<li class="list-obj row container">
			<a href="lists.html" class="list-el">
				<text class="col-xs-10">{{name}}</text>
				<text class="col-xs-10">{{quantity}}</text>
				<text class="col-xs-10">{{complete}}</text>
			</a>		
		</li>
		{{/each}}
	</ul>
	{{/each}}
	
	<div class="bottom-offset"></div>

	<footer class="tabs row">
		<div class="tab-icon col-xs-3">
			<span class="glyphicon glyphicon-list-alt"></span>
			<div class="tab-icon-name">lists</div>
		</div>
		<div class="tab-icon col-xs-3">
			<span class="glyphicon glyphicon-user"></span>
			<div class="tab-icon-name">friends</div>
		</div>
		<div class="tab-icon col-xs-3">
			<span class="glyphicon glyphicon-map-marker"></span>
			<div class="tab-icon-name">check in</div>
		</div>
		<div class="tab-icon col-xs-3">
			<span class="glyphicon glyphicon-heart"></span>
			<div class="tab-icon-name">trending</div>
		</div>
	</footer>

  <script src="https://code.jquery.com/jquery.js"></script>
  <script src="js/bootstrap.js"></script>
</body>
</html>
